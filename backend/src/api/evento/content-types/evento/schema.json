{
  "kind": "collectionType",
  "collectionName": "eventos",
  "info": {
    "singularName": "evento",
    "pluralName": "eventos",
    "displayName": "Evento"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titulo": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "titulo"
    },
    "descricao": {
      "type": "text"
    },
    "dataInicio": {
      "type": "date"
    },
    "local": {
      "type": "string"
    },
    "imagem": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "dataFim": {
      "type": "date"
    },
    "contato": {
      "type": "string"
    },
    "regulamento": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "modalidades": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::modalidade.modalidade",
      "inversedBy": "eventos"
    },
    "horario": {
      "type": "string"
    },
    "tipo_exibicao": {
      "type": "boolean"
    },
    "banner_carrossel_home": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
